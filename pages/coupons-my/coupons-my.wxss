/* pages/coupons-my/coupons-my.wxss */

page {
  background-color: #f5f5f5;
  padding: 30rpx;
  box-sizing: border-box;
}

.item {
  display: flex;
  padding: 10rpx;
  width: 100%;
  box-sizing: border-box;
  background-size: 100% 100%;
  color: #fff;
  line-height: 1;
  margin-bottom: 20rpx;
}

.left {
  flex: 0 0 350rpx;
  display: flex;
  align-items: center;
  width: 350rpx;
}

.item .icon {
  width: 50rpx;
  height: 50rpx;
  background-size: 100% 100%;
  margin: 25px 0 0 15px;
}

.right {
  flex: 1;
  background-color: #fff;
  text-align: center;
  display: flex;
  flex-direction: column;
  padding: 60rpx 0 40rpx 0;
}

.time {
  color: #666;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}

.btn {
  color: #5a9cd6;
  padding: 6rpx 20rpx;
  border: 1px solid #5a9cd6;
  border-radius: 30rpx;
  display: inline-block;
}
.btn.grey{
  color: #999;
  border: 1px solid #999;
}

.money {
  font-size: 140rpx;
  line-height: 1;
}

.name {
  font-family: "黑体";
  font-size: 50rpx;
  color: #333;
  margin-bottom: 20rpx;
}

.item.yellow {
  background-image: url("data:image/png;base64,");
}

.item.red {
  background-image: url("data:image/png;base64,");
}

.item.yellow .icon {
  background-image: url("data:image/png;base64,");
}

.item.red .icon {
  background-image: url("data:image/png;base64,");
}
