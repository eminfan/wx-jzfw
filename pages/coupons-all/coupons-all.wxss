/* pages/coupons-all/coupons-all.wxss */
 
page {
  background-color: #fff;
  padding: 30rpx;
  box-sizing: border-box;
}

.item {
  display: flex;
  padding: 40rpx 60rpx;
  width: 100%;
  box-sizing: border-box;
  background-size: 100% 100%;
  color: #fff;
  line-height: 1;
  margin-bottom: 20rpx;
}

.item .left {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.row1 {
  display: flex;
  align-items: flex-start;
}

.money {
  font-size: 140rpx;
  line-height: 1;
  margin-top: -20rpx;
  margin-left: 20rpx;
}

.name {
  font-family: "黑体";
  font-size: 50rpx;
}

.time {
}

.item .right {
  flex: 0 0 55rpx;
  width: 55rpx;
}

.item .icon {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background-size: 100% 100%;
  vertical-align: text-top;
  margin-bottom: 20rpx;
}

.item .btn {
  width: 55rpx;
  height: 220rpx;
  background-size: 100% 100%;
}

.item.yellow {
  background-image: url("data:image/png;base64,");
}

.item.yellow .icon {
  background-image: url("data:image/png;base64,");
}

.item.yellow .btn {
  background-image: url("data:image/png;base64,");
}

.item.red {
  background-image: url("data:image/png;base64,");
}

.item.red .icon {
  background-image: url("data:image/png;base64,");
}

.item.red .btn {
