/* pages/goods-cart/goods-cart.wxss */
@import "../../tpl/notask/notask.wxss";
.notask-wrap{
  margin-top: 200rpx;
}
view {
  box-sizing: border-box;
}

.item-box {
}

.items {
  margin-top:20rpx;
  width: 100%;
}

.item {
  position: relative;
  border-top: 2rpx solid #eee;
  height: 150rpx;
  overflow: hidden;
}

.item:last-child {
  border-bottom: 2rpx solid #eee;
}
.checkbox{
  width: 40rpx;
  height: 40rpx;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,")
}
.checkbox.checked{
  background-image: url("data:image/png;base64,")
}
.inner {
  position: absolute;
  height: 100%;
  top: 0;
}

.inner.content {
  background-color: #fff;
  width: 100%;
  z-index: 5;
  padding: 0 20rpx;
  transition: left 0.2s ease-in-out;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: flex;
  align-items: center;
}
.inner.content .checkbox{
  flex: 0 0 40rpx;
  margin-right:20rpx;
}
.inner.content .img {
flex:0 0 120rpx;
width:120rpx;
height:120rpx;
border-radius:10rpx;
margin-right:10rpx;

}

.inner.content .info {
flex:1;
height:100%;
display:flex;
flex-direction:column;
justify-content:space-around;

}

.inner.content .info text {
  display: block;
}
.inner.content .price{
  color: #f7504a;
}

.inner.del {
  background-color: #e64340;
  width: 180rpx;
  text-align: center;
  z-index: 4;
  right: 0;
  color: #fff;
  line-height:150rpx;
}

.item-icon {
  width: 64rpx;
  vertical-align: middle;
  margin-right: 16rpx;
}

.selnum {
  position: absolute;
  right: 20rpx;
  bottom:10rpx;
  width: 235rpx;
  height: 52rpx;
  border: 1rpx solid #ccc;
  display: flex;
}

.selnum .cut {
  width: 70rpx;
  height: 100%;
  text-align: center;
  line-height: 50rpx;
}

.selnum .number {
  flex: 1;
  height: 100%;
  text-align: center;
  line-height: 68.75rpx;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  float: left;
}

.selnum .add {
  width: 80rpx;
  height: 100%;
  text-align: center;
  line-height: 50rpx;
}

.pay-wrap{
  display: flex;
  position:fixed;
width:100%;
bottom:0;
background-color:#fff;
line-height: 100rpx;
}
.pay-wrap .left{
  flex: 1;
  display: flex;
  justify-content:space-between;
padding:0 20rpx;

}
.pay-wrap .right{
  flex: 0 0 150rpx;
  height: 100rpx;
  text-align: center;
  color: #fff;
  background-color: #f7504a;
}
.pay-wrap .checkbox{
  display: inline-block;
  vertical-align:middle;
margin-right:20rpx;

}
.pay-wrap text{
   vertical-align:middle; 

}
.pay-wrap .totle{
  color: #f7504a;
}